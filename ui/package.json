    "eslint": "8.44.0",
    "eslint-config-next": "^14.2.5",
    "eslint-plugin-local-rules": "^2.0.1",
    "framer-motion": "^12.18.1",
    "immer": "^10.0.2",
    "lodash": "^4.17.21",
    "lucide-react": "^0.408.0",
    "multer": "^1.4.5-lts.1",
    "next": "^15.0.3",
