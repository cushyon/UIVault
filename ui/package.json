{
  "name": "@drift-labs/dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev --turbo",
    "build": "next build",
    "clean": "rm -rf .next",
    "start": "next start",
    "lint": "eslint './**/*.{ts,tsx}'",
    "lint:fix": "yarn lint --fix",
    "prettify": "prettier --check './**/*.{ts,tsx}'",
    "prettify:write": "prettier --write './**/*.{ts,tsx}'",
    "typecheck": "tsc --noEmit",
    "convert-icons": "npx @svgr/cli --typescript --no-prettier --icon --out-dir ./src/components/icons ./public/icons && prettier --write './src/components/icons/*.{ts,tsx}'",
    "install-vercel": "sh ./scripts/install-vercel.sh",
    "build-vercel": "sh ./scripts/build-vercel.sh",
    "migrate-db": "bunx drizzle-kit generate && bunx drizzle-kit migrate"
  },
  "browser": {
    "fs": false,
    "os": false,
    "path": false,
    "crypto": false,
    "@microsoft/typescript-etw": false,
    "source-map-support": false,
    "inspector": false
  },
  "dependencies": {
    "@auth/dynamodb-adapter": "^2.4.1",
